		{
			"name": "Import Objects",
			"request": {
				"method": "PUT",
				"header": [],
				"body": {
					"mode": "raw",
					"raw": "[\n    {\n        \"kind\": \"user\",\n        \"apiVersion\": \"v1\",\n        \"metadata\": {\n            \"userId\": \"test-user\"\n        },\n        \"spec\": {\n            \"firstName\": \"Test\",\n            \"lastName\": \"User\",\n            \"email\": \"test@user.de\"\n        }\n    },\n    {\n        \"kind\": \"user\",\n        \"apiVersion\": \"v1\",\n        \"metadata\": {\n            \"userId\": \"test-user2\"\n        },\n        \"spec\": {\n            \"firstName\": \"Test\",\n            \"lastName\": \"User2\",\n            \"email\": \"test2@user.de\"\n        }\n    },\n    {\n        \"kind\": \"group\",\n        \"apiVersion\": \"v1\",\n        \"metadata\": {\n            \"name\": \"my-group\"\n        },\n        \"spec\": {\n            \"members\": [\n                \"test-user2\"\n            ]\n        }\n    }\n]",
					"options": {
						"raw": {
							"language": "json"
						}
					}
				},
				"url": {
					"raw": "http://localhost:8080/objects?objectCollection=my-test&owner=ich",
					"protocol": "http",
					"host": [
						"localhost"
					],
					"port": "8080",
					"path": [
						"objects"
					],
					"query": [
						{
							"key": "objectCollection",
							"value": "my-test"
						},
						{
							"key": "owner",
							"value": "ich"
						}
					]
				}
			},
			"response": []
		},
